# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                         controllers.Application.index
GET        /OqituvchilarAnketasi     controllers.Application.OqituvchilarAnketasi
GET        /OqituvchilarRoyhati      controllers.Application.OqituvchilarRoyhati
GET        /Xodimlar                 controllers.Application.Xodimlar
GET        /XodimlarRoyhati          controllers.Application.XodimlarRoyhati


# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file             controllers.Assets.at(path="/public", file)


